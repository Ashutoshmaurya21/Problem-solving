#include <iostream>
using namespace std;

int main() {
	int n;
	cin>>n;
	while(n--){
	    int a,b,c,d;
	    cin>>a>>b;
	    c=2*a;
	    d=5*b;
	    if(c==d){
	        cout<<"Either";
	    }
	    else if(c>d){
	        cout<<"Chocolate";
	    }
	    else{
	        cout<<"Candy";
	    }
	    cout<<endl;
	    
	}
	return 0;
}
